{
  "private": true,
  "type": "module",
  "name": "ani-altstore-source",
  "version": "0.0.0",
  "description": "Altstore source for Animeko",
  "homepage": "https://github.com/maxchang3/ani-altstore-source#readme",
  "bugs": "https://github.com/maxchang3/ani-altstore-source/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxchang3/ani-altstore-source.git"
  },
  "license": "MIT",
  "author": "maxchang3",
  "scripts": {
    "generate": "bun run src/index.ts",
    "lint": "biome check",
    "lint:fix": "biome check --write .",
    "prepare": "simple-git-hooks",
    "typecheck": "tsc --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "bun install --frozen-lockfile --ignore-scripts && bunx lint-staged"
  },
  "lint-staged": {
    "*": [
      "biome check --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@maxchang/biome-config": "^0.1.7",
    "@types/bun": "latest",
    "lint-staged": "^16.2.7",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.8"
}
